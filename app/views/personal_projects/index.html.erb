<h1>Listing personal_projects</h1>

<table>
  <tr>
    <th>Name</th>
    <th>Description</th>
    <th>Student</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @personal_projects.each do |personal_project| %>
  <tr>
    <td><%= personal_project.name %></td>
    <td><%= personal_project.description %></td>
    <td><%= personal_project.student_id %></td>
    <td><%= link_to 'Show', personal_project %></td>
    <td><%= link_to 'Edit', edit_personal_project_path(personal_project) %></td>
    <td><%= link_to 'Destroy', personal_project, method: :delete, data: { confirm: 'Are you sure?' } %></td>
  </tr>
<% end %>
</table>

<br />

